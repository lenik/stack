#!/bin/sh

# master-file

echo "Stop seccaj..."
    # initctl stop seccaj
    /etc/init.d/seccajctl stop

echo "Remove from sysv init"
    update-rc.d seccajctl remove

echo Remove /node/domains/seccaj

rm -fr /node/domains/seccaj
rm -f  /opt/java/glassfish/glassfish/domains/seccaj

rm -f  ~appserv/.seccaj.asadminpass

exit 0
