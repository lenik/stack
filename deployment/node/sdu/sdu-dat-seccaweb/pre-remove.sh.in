#!/bin/sh

# master-file

echo "Stop seccaw..."
    # initctl stop seccaw
    /etc/init.d/seccawctl stop

echo "Remove from sysv init"
    update-rc.d seccawctl remove

echo Remove /node/domains/seccaw

rm -fr /node/domains/seccaw
rm -f  /opt/java/glassfish/glassfish/domains/seccaw

rm -f  ~appserv/.seccaw.asadminpass
