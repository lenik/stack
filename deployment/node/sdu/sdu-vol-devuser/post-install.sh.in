#!/bin/sh
# master-file

if ! mount-ebs devuser /mnt/devuser; then
    echo "Failed to mount vol:devuser"
    exit 1
fi

if [ ! -h /home ]; then
    echo "Move anything under /home to vol:devuser"

    cd /
    cp -uRT /home /mnt/devuser
    mv home home.bak
    ln -snf /mnt/devuser home
fi

exit 0
