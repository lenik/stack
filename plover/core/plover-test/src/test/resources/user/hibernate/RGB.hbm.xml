<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="user.hibernate">
    <subclass name="RGB" extends="Color" discriminator-value="rgb">
        <property name="red" />
        <property name="green" />
        <property name="blue" />
    </subclass>
</hibernate-mapping>