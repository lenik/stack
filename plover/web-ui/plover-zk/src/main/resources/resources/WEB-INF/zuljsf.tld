<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:zk="http://www.zkoss.org/"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
    version="2.0">

    <uri>http://www.zkoss.org/jsf/zul</uri>

    <!-- General Purpose -->
    <tag><!--***** PageTag  *****-->
        <description>
    Define a page in a JSP tag.
    It is responsible for handling the lifecycle for all nested ZK
    components.
        </description>
        <name>page</name>
        <tag-class>org.zkoss.jsf.zul.tag.PageTag</tag-class>
        <body-content>scriptless</body-content>

        <attribute>
            <description>
    The CSS style. Default: null (none).
            </description>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
    The default script language. Default: Java.
            </description>
            <name>zscriptLanguage</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
    If false, this tag is ignored. Default: true.
            </description>
            <name>if</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
    If true, this tag is ignored. Default: false.
            </description>
            <name>unless</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag><!--***** ZScriptTag  *****-->
        <description>
    Define Zscript Tag, let user can declaire zscript fragment.
        </description>
        <name>zscript</name>
        <tag-class>org.zkoss.jsf.zul.tag.ZScriptTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <description>
    Always true in current.
            </description>
            <name>deferred</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>
        If true, this tag is ignored. Default: false.
            </description>
            <name>language</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag><!--***** AttributeTag  *****-->
        <description>
    Define Attribute Tag.
        </description>
        <name>attribute</name>
        <tag-class>org.zkoss.jsf.zul.tag.AttributeTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <description>
        Attribute Name.
            </description>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag><!--***** InitTag  *****-->
        <description>
    Define Init Tag.
        </description>
        <name>init</name>
        <tag-class>org.zkoss.jsf.zul.tag.InitTag</tag-class>
        <body-content>scriptless</body-content>
        <dynamic-attributes>true</dynamic-attributes>
        <attribute>
            <description>
        Attribute Name.
            </description>
            <name>use</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
